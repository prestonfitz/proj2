variable "aws_access_key" {
    description = "The AWS access key"
    type        = string
    default     = ""
}

variable "aws_secret_key" {
    description = "The AWS secret key"
    type        = string
    default     = ""
}

variable "donut_app_image" {
    description = "The image for the donut app"
    type        = string
    default     = "donut-app:latest"
}
